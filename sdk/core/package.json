{
  "name": "@zerolync/passkey-core",
  "version": "0.1.0",
  "description": "Core utilities and types for Zerolync Passkey SDK - cross-chain passkey authentication",
  "license": "MIT",
  "author": "Zerolync",
  "repository": {
    "type": "git",
    "url": "https://github.com/zerolync/zerolync-passkey-sdk",
    "directory": "sdk/core"
  },
  "keywords": [
    "passkey",
    "webauthn",
    "crypto",
    "wallet",
    "blockchain",
    "authentication"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
